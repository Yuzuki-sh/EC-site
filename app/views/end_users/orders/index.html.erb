<table>
    <tr>
        <td>注文日</td>
        <td>配送先</td>
        <td>注文商品</td>
        <td>支払金額</td>
        <td>ステータス</td>
        <td>注文詳細</td>
    </tr>
    <% @orders.each do |order| %>
        <%# 
        order_detail.class => ActiveRecord_Associations_CollectionProxy
        order_detail.first.class => OrderDetail 
        %>
        <%# order_detail = order_detail.first %>
        <tr>
            <td>
                <%= order.created_at.strftime('%Y/%m/%d %H:%M:%S') %>
            </td>
            <td>
                <%= order_detail.order.address %>
            </td>
            <td>
                <%= order_detail.order.total_fee %>
            </td>
            <td>
                <%= order_detail.order.orders_status %>
            </td>
            <td>
                <%= link_to "表示する",  end_users_order_path(order) %>
            </td>
        </tr>
    <% end %>
</table>